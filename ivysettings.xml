<?xml version="1.0"?>
<ivysettings>

  <settings defaultResolver="chained" defaultConflictManager="all"/>

  <resolvers>

    <!-- Chained resolver -->
    <chain name="chained">

      <!-- Algorithmia Maven Repository -->
      <ibiblio name="algorithmia-maven" m2compatible="true" root="${repo.url}/maven" changingPattern=".*SNAPSHOT" />

      <!-- Maven Central repo -->
      <ibiblio name="maven-central" m2compatible="true"/>

    </chain>

  </resolvers>

</ivysettings>
